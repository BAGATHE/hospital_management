<odoo>
  <record id="action_report_hospital_round_week" model="ir.actions.report">
    <field name="name">PDF Export - Weekly Round</field>
    <field name="model">hospital.round</field>
    <field name="report_type">qweb-pdf</field>
    <field name="report_name">nurse.report_hospital_round</field>
    <field name="binding_model_id" ref="hospital.model_hospital_round"/>
    <field name="binding_type">report</field>
    <field name="print_report_name">'Round_Week_Report' + (object.round_date or 'unknown')</field>
  </record>
</odoo>
