<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="demo_disease_1" model="hospital.disease">
        <field name="name">Influenza</field>
        <field name="description">A common viral infection that can be deadly, especially in high-risk groups.</field>
        <field name="contagiousness">yes</field>
    </record>

    <record id="demo_disease_2" model="hospital.disease">
        <field name="name">Diabetes</field>
        <field name="description">A chronic condition that affects the way the body processes blood sugar.</field>
        <field name="contagiousness">no</field>
    </record>

    <record id="demo_disease_3" model="hospital.disease">
        <field name="name">Tuberculosis</field>
        <field name="description">A potentially serious infectious disease that mainly affects the lungs.</field>
        <field name="contagiousness">yes</field>
    </record>

    <record id="demo_disease_4" model="hospital.disease">
        <field name="name">Hypertension</field>
        <field name="description">A condition in which the force of the blood against the artery walls is too high.</field>
        <field name="contagiousness">no</field>
    </record>
</odoo>